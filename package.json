{
  "name": "hoshi",
  "version": "1.0.0",
  "description": "A starboard bot",
  "main": "hoshi.js",
  "scripts": {
    "test": "eslint ./"
  },
  "eslintConfig": {
    "extends": "aqua",
    "rules": {
      "max-len": [
        "error",
        120,
        2,
        {
          "ignoreStrings": true,
          "ignoreTemplateLiterals": true
        }
      ],
      "prefer-destructuring": [
        "off"
      ],
      "quotes": [
        "error",
        "single"
      ]
    }
  },
  "contributors": [
    "1Computer",
    "vzwGrey"
  ],
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-aqua": "^2.0.1"
  },
  "dependencies": {
    "chalk": "^2.2.0",
    "discord-akairo": "github:1computer1/discord-akairo",
    "discord.js": "github:hydrabolt/discord.js",
    "moment": "^2.19.1",
    "pg": "^6.4.2",
    "raven": "^2.2.1",
    "sequelize": "^4.15.0"
  }
}
